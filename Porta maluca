            break;
        case "2":
            document.write("<img src ='/IMG/mike.jfif' width = '70%'>")
            
            break;
        case "3":
            document.write("<img src ='/IMG/sulivan.jfif' width = '70%'>")
            
            break;
        case "4":
            document.write("<img src ='/IMG/oscar.jfif' width = '170%'>")
            
            break;
        
    }


</script>

